{
  "port": 3002,
  "metadata": true,
  "dependencies": true,
  "assetpath": "/dev/AlphaHelp/helpserver/",
  "xslt" : "xform.xslt",
  "altTocs": ["/Api/"],
  "defaultPathMetadata" : [{ "name" : "/Api/" , "metadata" : { "tags" : "common", "status" : "accept" } }] ,  
  "source": "/dev/AlphaHelp/helpfiles/",
  "logoHref": "http://www.alphasoftware.com/",
  "pageTemplate" : "pageTemplate.html",
  "searchTemplate" : "searchTemplate.html",    
  "proxy": "http://www.alphasoftware.com/testdoc/",
  "generated": "/dev/AlphaHelp/generated/",
  "templatesLocation": "/dev/AlphaHelp/templates/",
  "defaultPage": "/web/main",
  "defaultFilter": "nofilter",
  "responseHeader": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Headers": "*",
    "Access-Control-Allow-Methods": "PUT, GET, POST, DELETE, OPTIONS",
    "Access-Control-Allow-Credentials": true
  },
  "keywords": [
    {
      "exclusive": true,
      "group": [
        "server",
        "client",
        "desktop"
      ]
    },
    {
      "exclusive": true,
      "group": [
        "api",
        "design",
        "debug",
        "deploy"
      ]
    },
    "security",
    {
      "exclusive": true,
      "group": [
        "ref",
        "tut",
        "guide",
        "index"
      ]
    },
    {
      "group": [
        "xbasic",
        "javascript",
        "actionxb",
        "actionjs"
      ]
    },
    "schema",
    "relation",
    "rules",
    {
      "group": [
        "operation",
        "form",
        "browse",
        "xdialog",
        "report",
        "letter",
        "label",
        "lcontrol",
        "xdcontrol",
        "wcontrol",
        "controlpanel",
        "adb",
        "xdcommand"
      ]
    },
    "grid",
    "ux",
    "component",
    "chart",
    {
      "group": [
        "sql",
        "dbf",
        "json",
        "bson",
        "set"
      ]
    },
    "ajax",
    "dotnet",
    "spreadsheet",
    "rtf",
    "html",
    "a5w",
    "css",
    "sass",
    "asports",
    "aex",
    "genie"
  ],
  "ignoreItems": [
    "images"
  ],
  "search": {
    "provider": "elasticsearch"
  },
  "configurations": {
    "web": {
      "filter_name": "web",
      "filter": {
        "metadata.tags": "common,web"
      },
      "topPage" : "web_toc.html",
      "editTOC": {
        "move": [
          {
            "from": "/Client",
            "to": "/"
          },
          {
            "from": "/Server",
            "to": "/"
          },
          {
            "from": "/Desktop",
            "to": "/"
          }
        ]
      } 
    },
    "desktop": {
      "filter_name": "desktop",
      "filter": {
        "metadata.tags": "common,desktop"
      },
      "topPage" : "desktop_toc.html",
      "editTOC": {
        "move": [
          {
            "from": "/Client",
            "to": "/"
          },
          {
            "from": "/Server",
            "to": "/"
          },
          {
            "from": "/Desktop",
            "to": "/"
          },
          {
            "from": "/Guide",
            "to": "/_1_Guide"
          },          
          {
            "from": "/Tutorial",
            "to": "/_2_Tutorial"
          },          
          {
            "from": "/Reference",
            "to": "/_3_Reference"
          },
          {
            "from": "/Index/Release Notes",
            "to": "/_5_Release Notes"
          },
          {
            "from": "/Index",
            "to": "/_4_Index"
          }
        ]
      }
    },
    "review": {
      "filter_name": "review",
      "filter": {
        "metadata.tags": "review"
      },
      "isAdmin": true
    },
    "reviewre": {
      "filter_name": "reviewre",
      "filter": {
        "metadata.tags": "review"
      },
      "isAdmin": true,
      "editTOC": {
        "move": [
          {
            "from": "/Client",
            "to": "/"
          },
          {
            "from": "/Server",
            "to": "/"
          },
          {
            "from": "/Desktop",
            "to": "/"
          }
        ]
      }
    },
    "admin": {
      "isAdmin": true
    },
    "missing": {
      "filter_name": "missing",
      "filter": {
        "missing": {
          "field": "metadata"
        }
      }
    },
    "nostatus": {
      "filter_name": "nostatus",
      "filter": {
        "missing": {
          "field": "status"
        }
      }
    },
    "notag": {
      "filter_name": "notag",
      "filter": {
        "missing": {
          "field": "metadata.tags"
        }
      }
    },
    "nofilter": {
      "filter_name": "nofilter",
      "filter": {
        "exists": {
          "field": "metadata"
        }
      }
    },
    "reorg": {
      "filter_name": "reorg",
      "filter": {
        "exists": {
          "field": "metadata"
        }
      },
      "editTOC": {
        "move": [
          {
            "from": "/Client",
            "to": "/"
          },
          {
            "from": "/Server",
            "to": "/"
          },
          {
            "from": "/Desktop",
            "to": "/"
          }
        ]
      }
    }
  }
}