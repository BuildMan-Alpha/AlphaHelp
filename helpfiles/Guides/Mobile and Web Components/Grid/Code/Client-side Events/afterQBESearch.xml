<page api="js">
    <shortlink>grid component client side afterqbesearch event</shortlink>
    <topic>afterQBESearch</topic>
    <description>Fires after the user has performed a Query by Example (QBE) search.</description>
    <!--<discussion></discussion>-->
    <arguments>
        <argument>
            <name>e</name>
            <type>object</type>
            <description>An object passed to the event that contains the following properties:</description>
            <properties>
                <property>
                    <name>qbeType</name>
                    <type>string</type>
                    <description>The search option selected from the QBE menu. Can be one of the following:</description>
                    <section>
                        <list bullet="true">
                            <item>
                                <name>Exact match</name>
                            </item>
                            <item>
                                <name>Starts with</name>
                            </item>
                            <item>
                                <name>Ends with</name>
                            </item>
                            <item>
                                <name>Contains</name>
                            </item>
                            <item>
                                <name>Greater than</name>
                            </item>
                            <item>
                                <name>Greater than or equal to</name>
                            </item>
                            <item>
                                <name>Less than</name>
                            </item>
                            <item>
                                <name>Less than or equal to</name>
                            </item>
                            <item>
                                <name>Wildcard</name>
                            </item>
                            <item>
                                <name>QBE Syntax</name>
                            </item>
                            <item>
                                <name>Is blank</name>
                            </item>
                            <item>
                                <name>Is null</name>
                            </item>
                            <item>
                                <name>Is not blank</name>
                            </item>
                            <item>
                                <name>is not null</name>
                            </item>
                            <item>
                                <name>Clear All Filters</name>
                            </item>
                        </list>
                    </section>
                </property>
            </properties>
        </argument>
    </arguments>
    <!--<sections>
        <section>
            <example code="js"><![CDATA[]]></example>
        </section>
    </sections>-->
</page>