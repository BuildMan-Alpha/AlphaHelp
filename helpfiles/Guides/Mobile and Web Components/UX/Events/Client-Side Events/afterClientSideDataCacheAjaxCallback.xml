<page build="0">
    <shortlink>uxevent afterclientsidedatacacheajaxcallback</shortlink>
    <topic>afterClientSideDataCacheAjaxCallback</topic>
    <description>Fires after the Ajax Callback to fetch data for the Client-side data cache completes.</description>
    <arguments>
        <argument>
            <name>e</name>
            <type>Object</type>
            <description>
                <p>An object with the following properties:</p>
            </description>
            <properties>
                <property>
                    <name>item</name>
                    <type>string</type>
                    <description>The Client-side data cache item that was loaded.</description>
                </property>
                <property>
                    <name>location</name>
                    <type>string</type>
                    <description>The location where the item was stored. For example, "Local Storage".</description>
                </property>
            </properties>
        </argument>
    </arguments>
    <see>
        <ref link="uxevent onBeforeClientSideDataCacheRead">onBeforeClientSideDataCacheRead</ref>
        <ref link="uxevent afterControlBasedOnClientSideDataCacheRefreshed">afterControlBasedOnClientSideDataCacheRefreshed</ref>
    </see>
</page>