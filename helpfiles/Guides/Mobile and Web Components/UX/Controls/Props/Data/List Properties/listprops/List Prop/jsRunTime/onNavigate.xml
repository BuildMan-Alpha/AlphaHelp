<page>
    <shortlink>ux list clientside event onnavigate</shortlink>
    <topic>onNavigate Event</topic>
    <description>Fires when after List has been navigated (i.e. scrolled).</description>
    <arguments>
        <argument>
            <name>e</name>
            <type>object</type>
            <description>An object that contains the following properties:</description>
            <properties>
                <property>
                    <name>startIndex</name>
                    <type>numeric</type>
                    <description>A zero-based logical row number of the first row in view.</description>
                </property>
                <property>
                    <name>endIndex</name>
                    <type>numeric</type>
                    <description>A zero-based logical row number of the last row in view.</description>
                </property>
            </properties>
        </argument>
    </arguments>
    <see>
        <ref link="list control ux api">List Control JavaScript Methods</ref>
            <ref link="ux%20list%20events">List Control Client-side Events</ref>
        <ref link="ux%20list%20csevent">Detail View Client-side Events</ref>
        <ref link="ux%20list%20builder%20jsruntime%20properties">Javascript - Run Time</ref>
        <ref link="ux%20list%20builder%20jsrendertime%20properties">Javascript - Render Time</ref>
    </see>
</page>