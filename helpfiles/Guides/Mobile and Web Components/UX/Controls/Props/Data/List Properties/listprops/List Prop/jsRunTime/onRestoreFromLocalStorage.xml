    <topic>onRestoreFromLocalStorage Event</topic>
    <description>Fires after the List data has been restored from Local Storage.</description>
    <arguments>
        <argument>
            <name>e</name>
            <type>object</type>
            <description>An object that contains the following properties:</description>
            <properties>
                <property>
                    <name>userData</name>
                    <type>object</type>
                    <description>Contains any data that was set in the onBeforePersistToLocalStorage event.</description>
                </property>
            </properties>
        </argument>
    </arguments>
