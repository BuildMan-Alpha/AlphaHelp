   <arguments>
     <argument>
         <name>script_name</name>
         <description>Character</description>
     </argument>
     <argument>
         <name>new_name</name>
         <description>Character</description>
     </argument>
     <argument>
         <name>Password</name>
         <description>Character</description>
     </argument>
     <argument>
         <name>annotate</name>
         <description>Logical</description>
     </argument>
   </arguments>
   <description>Rename a script (must include password if renaming a protected script).</description>
</page>