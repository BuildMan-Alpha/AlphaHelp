<page>
	<group>JSON Functions</group>
	<topic>
		json_enumerate()
	</topic>
	<syntax>
		json_enumerate(c jsontext)
	</syntax>

	<description>
		Gets all of the top level property names.
	</description>


	<example>
		? json_enumerate("{ \"firstname\" : \"joe\" , \"lastname\" : \"plumber\" }") = firstname lastname
	</example>


	<see>
		<ref>argumentsfromjson()</ref>
		<ref>crlf_to_json()</ref>
		<ref>JSON Data</ref>
	</see>
</page>