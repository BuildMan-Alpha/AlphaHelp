<page>
	
	<topic>json_enumerate()</topic>
	
	<description>Gets all of the top level property names.</description>

	<syntax>
		json_enumerate(c jsontext)
	</syntax>


	<example>
		? json_enumerate("{ \"firstname\" : \"joe\" , \"lastname\" : \"plumber\" }") = firstname lastname
	</example>


