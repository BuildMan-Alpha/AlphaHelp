<page api="xb">
    <shortlink>api submit function</shortlink>
    <topic>submit Function</topic>
    <description>Submit a command to run in another session.</description>

    <syntax>A SUBMIT(N session,C command)</syntax>
    <arguments>
        <argument>
            <name>session</name>
            <type>N</type>
            <description>The session.</description>
        </argument>
        <argument>
            <name>command</name>
            <type>C</type>
            <description>The command.</description>
        </argument>
    </arguments>
    <terms>session,function,submit</terms>
    <limitations>Desktop applications only.</limitations>
    <see>
        <ref link="api objects context session object">Context.Session</ref>
    </see>
</page>