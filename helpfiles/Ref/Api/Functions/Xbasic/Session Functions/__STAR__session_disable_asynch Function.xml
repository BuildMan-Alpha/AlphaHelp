<page api="xb">
  <shortlink>api star session disable asynch function</shortlink>
  <topic>*session_disable_asynch Function</topic>
  <description>Force session to execute all code in the foreground thread.</description>

  <syntax>V *SESSION_DISABLE_ASYNCH([N session_id])</syntax>
  <arguments>
    <argument>
      <name>session_id</name>
      <description>Numeric</description>
    </argument>
  </arguments>
  <terms>session,function</terms>
  <limitations>Desktop applications only.</limitations>
  <see>
    <ref link="api objects context session object">Context.Session</ref>
  </see>


</page>