<page api="xb">
    <shortlink>api evaluate shared private function</shortlink>
    <topic>evaluate_shared_private Function</topic>
    <description>Evaluate code in its own session, but with shared variables (passed in).</description>

    <syntax>C EVALUATE_SHARED_PRIVATE(C Code,P vars)</syntax>
    <arguments>
        <argument>
            <name>Code</name>
            <description>Character</description>
        </argument>
        <argument>
            <name>vars</name>
            <description>Pointer</description>
        </argument>
    </arguments>

    <terms>session,function,pointer</terms>
    <limitations>Desktop applications only.</limitations>
    <see>
        <ref link="api objects context session object">Context.Session</ref>
    </see>


</page>