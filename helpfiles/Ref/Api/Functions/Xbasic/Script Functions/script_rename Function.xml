<page api="xb">
    <shortlink>api script rename function</shortlink>
    <topic>script_rename Function</topic>
    <description>Rename a script (must include password if renaming a protected script).</description>
    <syntax>C SCRIPT_RENAME(C script_name,C new_name[,C Password[,L annotate]])</syntax>
    <arguments>
        <argument>
            <name>script_name</name>
            <type>C</type>
            <description>Character</description>
        </argument>
        <argument>
            <name>new_name</name>
            <type>C</type>
            <description>Character</description>
        </argument>
        <argument optional="true">
            <name>Password</name>
            <type>C</type>
            <description>Character</description>
        </argument>
        <argument optional="true">
            <name>annotate</name>
            <type>L</type>
            <description>Logical</description>
        </argument>
    </arguments>
    <terms>script,function,logical,password</terms>
</page>