<page api="xb">
    <shortlink>api regex literal insert function</shortlink>
   <topic>regex_literal_insert Function</topic>
     <description>Insert the literal characters into an undecorated string.</description>


   <syntax>C REGEX_LITERAL_INSERT(text as c,RegEx as c[,options as c])</syntax>
   <arguments>
     <argument>
         <name>text</name>
         <description>Character</description>
     </argument>
     <argument>
         <name>RegEx</name>
         <description>Character</description>
     </argument>
     <argument>
         <name>options</name>
         <description>Character</description>
     </argument>
   </arguments>
 <example>
? regex_literal_insert("8004511018","\([0-9][0-9][0-9]\) [0-9][0-9][0-9] [0-9][0-9][0-9][0-9]")
= "(800) 451 1018"
</example>

