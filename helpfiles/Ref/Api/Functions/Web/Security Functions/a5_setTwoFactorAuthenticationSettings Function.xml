<page api="xb" build="5786">
    <shortlink>a5_setTwoFactorAuthenticationSettings function</shortlink>
    <topic>a5_setTwoFactorAuthenticationSettings Function</topic>
    <description>
        <p>Sets the two-factor authentication options.</p>
    </description>
    <prototypes>
        <prototype>p result = a5_setTwoFactorAuthenticationSettings(c userid, c json)</prototype>
    </prototypes>
    <arguments>
        <argument>
            <name>userid</name>
            <type>C</type>
            <description>
                <p>The user's user id</p>
            </description>
        </argument>
        <argument>
            <name>json</name>
            <type>C</type>
            <description>
                <p>JSON string defining the two-factor options. For example:</p>
                <p>
                    <example code="json"><![CDATA[{
    "requireTwoFactorAuthentication": true,
    "method": "Google Authenticator",
    "mobilePhone": "",
    "email": ""
}]]></example>
                </p>
            </description>
        </argument>
    </arguments>
    <returns>
        <return>
            <name>result</name>
            <types>
                <type>P</type>
            </types>
            <description>
                <p>An object with error and errorText (if error is .t.) as properties.</p>
            </description>
        </return>
    </returns>
    <see>
        <ref link="two factor auth">Two-factor Authentication</ref>
    </see>
    <links>
        <link link="">json_parse()</link>
    </links>
</page>