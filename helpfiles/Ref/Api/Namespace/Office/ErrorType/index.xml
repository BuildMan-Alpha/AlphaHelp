<page api="xb">
    <shortlink>office errortype enumeration</shortlink>
    <topic>Office::ErrorType Enumeration</topic>
    <description>List of Office error types that can be set in Office::Spreadsheet cells.</description>
    <sections>
        <section>
            <list>
                <item>
                    <name-title>Error</name-title>
                    <description-title>Description</description-title>
                </item>
                <item>
                    <name>*[xb:Office::ErrorType::Null]*</name>
                    <description>Value = 0. "#NULL!" ' 0x00  </description>
                </item>
                <item>
                    <name>*[xb:Office::ErrorType::DivideByZero]*</name>
                    <description>Value = 7. "#DIV/0!" ' 0x07  </description>
                </item>
                <item>
                    <name>*[xb:Office::ErrorType::Value]*</name>
                    <description>Value = 15. "#VALUE!" ' 0x0F  </description>
                </item>
                <item>
                    <name>*[xb:Office::ErrorType::Reference]*</name>
                    <description>Value = 23. "#REF!" ' 0x17  </description>
                </item>
                <item>
                    <name>*[xb:Office::ErrorType::Name]*</name>
                    <description>Value = 29. "#NAME?" ' 0x1D  </description>
                </item>
                <item>
                    <name>*[xb:Office::ErrorType::Number]*</name>
                    <description>Value = 36. "#NUM!" ' 0x24  </description>
                </item>
                <item>
                    <name>*[xb:Office::ErrorType::NA]*</name>
                    <description>Value = 42. "#N/A" ' 0x2A  </description>
                </item>
                <item>
                    <name>*[xb:Office::ErrorType::NoError]*</name>
                    <description>Value = 255. "No error." ' 0xFF </description>
                </item>
            </list>
        </section>
    </sections>
</page>