<page>
	<shortlink>api A5Storage DataItem Class</shortlink>
	<topic>A5Storage::DataItem Class</topic>
	<description>A reference to an object in a Storage container.  A DataItem does not hold any data but has methods and properties that can be used to access the object itself and to get information about it without reading the object into memory.</description>
	<properties>
		<property readonly="true">
			<name>AbsolutePath</name>
			<type>C</type>
			<description>URL to reference the item directly in a browser.</description>
		</property>
		<property  readonly="true">
			<name>CallResult</name>
			<type>CallResult</type>
			<description>String CallResult property</description>
		</property>
		<property generated="true">
			<name>ContentType</name>
			<type>C</type>
			<description>String ContentType property</description>
		</property>
		<property  readonly="true">
			<name>Exists</name>
			<type>L</type>
			<description>String Exists property</description>
		</property>
		<property  readonly="true">
			<name>ModifiedTime</name>
			<type>T</type>
			<description>String ModifiedTime property</description>
		</property>
		<property  readonly="true">
			<name>Name</name>
			<type>C</type>
			<description>String Name property</description>
		</property>
		<property  readonly="true">
			<name>Size</name>
			<type>N</type>
			<description>String Size property</description>
		</property>
	</properties>
