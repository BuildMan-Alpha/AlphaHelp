    <topic parent="SQL::TableInfo" parentType="class" elementName="MoveIndex Method">SQL::TableInfo::MoveIndex Method</topic>
    <description>Move a block of Index items.</description>
    <prototypes>
        <prototype>DIM result AS L = MoveIndex(MoveTo as N, MoveFrom as N [, ItemsToCopy as N = 1])</prototype>
    </prototypes>
    <arguments>
        <argument>
            <name>MoveTo</name>
            <type>N</type>
            <description>
                <p>Where to move the index.</p>
            </description>
        </argument>
        <argument>
            <name>MoveFrom</name>
            <type>N</type>
            <description>
                <p>The index to move.</p>
            </description>
        </argument>
        <argument optional="true">
            <name>ItemsToCopy</name>
            <type>N</type>
            <description>
                <p>Default = *[xb:-1]*. The number of items to copy.</p>
            </description>
        </argument>
    </arguments>
    <returns>
        <return>
            <name>result</name>
            <types>
                <type>L</type>
            </types>
            <description>
                <p>Returns *[xb:.t.]* if the operation succeeds. Otherwise, *[xb:.f.]*.</p>
            </description>
        </return>
    </returns>
