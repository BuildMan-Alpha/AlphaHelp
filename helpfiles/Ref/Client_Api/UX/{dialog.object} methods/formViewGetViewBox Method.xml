<page api="js">
    <shortlink>ux component formviewgetviewbox method</shortlink>
    <topic parent="{dialog.object}" parentType="object" elementName="formViewGetViewBox Method">{dialog.object}.formViewGetViewBox Method</topic>
    <description>Gets a pointer to the ViewBox control used in the FormView control.</description>
    <prototypes>
        <prototype>var viewbox = {dialog.object}.formViewGetViewBox(UXFormViewControlId);</prototype>
    </prototypes>
    <arguments>
        <argument>
            <name>UXFormViewControlId</name>
            <type>string</type>
            <description>The FormView control's ID.</description>
        </argument>
    </arguments>
    <returns>
        <return>
            <name>viewbox</name>
            <types>
                <type>A5.ViewBox</type>
            </types>
            <description>Returns the FormView's ViewBox. See [A5.ViewBox] for more information about this object.</description>
        </return>
    </returns>
    <example code="js"><![CDATA[var vbObj = {dialog.object}.formViewGetViewBox('FORM_1');]]></example>
    <links>
        <link link="api%20client%20api%20a5%20viewbox%20class">A5.ViewBox</link>
    </links>
    <see>
        <ref link="api%20client%20api%20a5%20viewbox%20class">A5.ViewBox Class</ref>
    </see>
</page>